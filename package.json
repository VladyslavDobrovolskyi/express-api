    "type": "git",
    "url": "git+https://github.com/VladyslavDobrovolskyi/express-api.git"
  },
  "keywords": [
    "express",
    "mongoose",
    "api"
  ],
  "author": "VladyslavDobrovolskyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VladyslavDobrovolskyi/express-api/issues"
  },
  "homepage": "https://github.com/VladyslavDobrovolskyi/express-api#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^7.0.0",
    "mongoose": "^7.3.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^8.43.0",
    "nodemon": "^2.0.22"
